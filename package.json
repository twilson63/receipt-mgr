{
  "name": "receipt-mgr",
  "version": "0.0.0",
  "script": {
    "build": "atomify -d -s false -e app/index -o www/bundle"
  },
  "atomify": {
    "js": {
      "entry": "app/index.js",
      "output": "www/bundle.js"
    },
    "css": {
      "entry": "app/index.css",
      "output": "www/bundle.css",
      "assets": {
        "prefix": "fonts/",
        "dest": "www/fonts"
      }
    }
  },
  "dependencies": {
    "domify": "^1.2.2",
    "es3ify": "^0.1.3",
    "underscore": "^1.6.0"
  }
}
