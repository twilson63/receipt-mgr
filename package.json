{
  "name": "receipt-mgr",
  "version": "0.0.0",
  "scripts": {
    "install": "bower install",
    "start": "atomify -d"
  },
  "atomify": {
    "js": {
      "entry": "app/index.js",
      "output": "www/bundle.js"
    },
    "css": {
      "entry": "app/index.css",
      "output": "www/bundle.css",
      "assets": {
        "prefix": "/fonts/",
        "dest": "www/fonts"
      }
    },
    "server": {
      "lr": true,
      "path": "index.html",
      "st": {
        "path": "www"
      }
    }
  },
  "dependencies": {
    "domify": "^1.2.2",
    "es3ify": "^0.1.3",
    "underscore": "^1.6.0"
  }
}
