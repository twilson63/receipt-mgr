{
  "name": "receipt-mgr",
  "version": "0.0.33",
  "scripts": {
    "install": "bower install",
    "start": "atomify -d",
    "test": "tap app/**/*_test.js"
  },
  "atomify": {
    "js": {
      "entry": "app/index.js",
      "output": "www/bundle.js"
    },
    "css": {
      "entry": "app/index.css",
      "output": "www/bundle.css",
      "assets": {
        "prefix": "fonts/",
        "dest": "www/fonts"
      }
    },
    "server": {
      "lr": true,
      "path": "index.html",
      "st": {
        "path": "www",
        "cache": false
      }
    }
  },
  "dependencies": {
    "domify": "^1.2.2",
    "es3ify": "^0.1.3",
    "underscore": "^1.6.0"
  },
  "devDependencies": {
    "tap": "^0.4.9"
  },
  "description": "Receipt Manager is a mobile application that allows you to capture and track receipts for expenses.",
  "main": "app/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/twilson63/receipt-mgr.git"
  },
  "author": "Tom Wilson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twilson63/receipt-mgr/issues"
  },
  "homepage": "https://github.com/twilson63/receipt-mgr"
}
